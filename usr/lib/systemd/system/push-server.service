[Unit]
Description=RTC service
After=redis.target

[Service]
User=bitrix
Group=bitrix
Type=forking
ExecStart=/etc/init.d/push-server-multi systemd_start
ExecStop=/etc/init.d/push-server-multi stop
WorkingDirectory=/opt/push-server
Restart=always
RestartSec=30
PrivateTmp=false

[Install]
WantedBy=multi-user.target
